<div id="rankings" class="row">

  <%= render 'navlist' %>

  <div id="rankings-index" class="span5 offset1">

    <% if @rankings.blank? %>
    <div class="alert">
      <strong><%= t '.no_rankings' %></strong>
    </div>
    <% else %>

      <div>
        <%= paginate @rankings %>
      </div>

      <%= render partial: "teams", object: @rankings %>

    <% end %>
  </div>
</div>
